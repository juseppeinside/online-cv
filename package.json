{
  "name": "online-cv",
  "private": true,
  "version": "1.0.4",
  "type": "module",
  "description": "Modern interactive CV built with React, TypeScript, GSAP, and Tailwind CSS",
  "author": {
    "name": "Shkividorov Maxim",
    "email": "maksimshkiv@mail.ru",
    "url": "https://github.com/juseppeinside"
  },
  "keywords": [
    "cv",
    "react",
    "typescript",
    "gsap",
    "tailwindcss",
    "vite"
  ],
  "license": "MIT",
  "scripts": {
    "dev": "vite --host",
    "build": "tsc -b && vite build",
    "preview": "vite preview",
    "typecheck": "tsc --noEmit -p tsconfig.app.json",
    "lint": "ultracite lint",
    "format": "ultracite format",
    "prepare": "husky",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage"
  },
  "dependencies": {
